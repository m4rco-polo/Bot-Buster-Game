[sub_resource type="GDScript" id="GDScript_k4wox"]
script/source = "extends AnimationPlayer
var speed
const WALK_SPEED = 5.0
const SPRINT_SPEED = 8.0
const JUMP_VELOCITY = 4.5
const SENSITIVITY = 0.015
const SLIDE_SPEED = 15
# Get the gravity from the project settings to be synced with RigidBody nodes.
var gravity = 9.8

# bob variables
const BOB_FREQ = 2.0
const BOB_AMP = 0.08
var t_bob = 0.0

# fov variables
var fov = 75
const FOV_CHANGE = 1.5

@onready var head = $Head
@onready var camera = $Head/Camera3D
@onready var particles = $particles

# Called when the node enters the scene tree for the first time.
func _ready():
	if Input.is_action_just_pressed(\"slide\") and Input.is_action_pressed(\"sprint\"):
		play(\"slide\")
		particles.emitting = true
	if Input.is_action_just_pressed(\"slide\") and Input.is_action_pressed(\"sprint\") and is_on_floor():
		speed = SLIDE_SPEED
	if Input.is_action_just_released(\"slide\"):
		play(\"RESET\")
		particles.emitting = false
		speed = SPRINT_SPEED


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	pass
"

